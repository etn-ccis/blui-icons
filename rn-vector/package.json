{
  "name": "@pxblue/react-native-vector-icons",
  "title": "React Native PX Blue Vector Icons",
  "version": "1.0.0",
  "description": "PX Blue vector icons for use in React Native projects, based on react-native-vector-icons",
  "main": "index.js",
  "files": [
    "android",
    "CHANGELOG.md",
    "fonts.gradle",
    "Fonts",
    "GlyphMaps",
    "index.js",
    "index.d.ts",
    "ios",
    "LICENSE",
    "RNPXBVectorIcons.podspec",
    "README.md",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
