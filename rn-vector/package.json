  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-native": ">=0.60.0-rc.0 <1.0.x",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "react": "^16.9.0",
    "react-native": "^0.61.5",
