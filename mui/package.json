{
    "name": "@brightlayer-ui/icons-mui",
    "version": "2.7.0-beta.0",
    "description": "Brightlayer UI icons for Material UI",
    "main": "./index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brightlayer-ui/icons.git"
    },
    "author": "Brightlayer UI <brightlayer-ui@eaton.com>",
    "license": "BSD-3-Clause",
    "peerDependencies": {
        "@material-ui/core": ">=3.3.1",
        "@material-ui/icons": ">=3.0.1"
    },
    "scripts": {
        "build": "bash ./scripts/generateIcons.sh",
        "link:components": "bash ./scripts/linkComponents.sh",
        "install:dependencies": "yarn && cd scripts/mui-icon-builder && yarn && cd ../.. && cd demos/showcase && yarn && cd ..",
        "start:showcase": "yarn install:dependencies && yarn build && yarn link:components && cd demos/showcase && yarn start",
        "start:storybook": "echo 'No storybook demo yet'",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --write",
        "start": "yarn start:showcase",
        "test": "./scripts/buildTest.sh",
        "publish:package": "cd dist && set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
        "tag:package": "cd dist && set npm_config_yes=true && npx -p @brightlayer-ui/tag blui-tag"
    },
    "bugs": {
        "url": "https://github.com/brightlayer-ui/icons/issues"
    },
    "prettier": "@brightlayer-ui/prettier-config",
    "homepage": "https://github.com/brightlayer-ui/icons/mui#readme",
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/node": "7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "@brightlayer-ui/icons-svg": "latest",
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "prettier": "^2.3.1"
    },
    "keywords": [
        "icons",
        "material",
        "material-ui",
        "brightlayer-ui"
    ]
}
