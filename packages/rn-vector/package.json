{
    "name": "@brightlayer-ui/react-native-vector-icons",
    "title": "React Native Brightlayer UI Vector Icons",
    "version": "2.4.0-alpha.0",
    "description": "Brightlayer UI vector icons for use in React Native projects, based on react-native-vector-icons",
    "main": "index.js",
    "files": [
        "android",
        "CHANGELOG.md",
        "fonts.gradle",
        "Fonts",
        "GlyphMaps",
        "index.js",
        "index.d.ts",
        "ios",
        "LICENSE",
        "RNBLUIVectorIcons.podspec",
        "README.md",
        "!android/build",
        "!ios/build"
    ],
    "scripts": {
        "test": "echo \"No test specified yet\" && exit 0",
        "build": "yarn && bash ./scripts/buildIcons.sh",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --write",
        "prettier:check": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --check",
        "publish:package": "cross-env npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
        "tag:package": "cross-env npm_config_yes=true && npx -p @brightlayer-ui/tag blui-tag -s -blui-react-native-vector-icons"
    },
    "bugs": {
        "url": "https://github.com/etn-ccis/blui-icons/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/etn-ccis/blui-icons.git",
        "baseUrl": "https://github.com/etn-ccis/blui-icons/tree/master/react-native-vector-icons"
    },
    "keywords": [
        "react-native",
        "Brightlayer UI",
        "brightlayer-ui",
        "icons",
        "vector",
        "power",
        "eaton"
    ],
    "author": {
        "name": "Brightlayer UI",
        "email": "brightlayer-ui@eaton.com"
    },
    "license": "BSD-3-Clause",
    "licenseFilename": "LICENSE",
    "readmeFilename": "README.md",
    "peerDependencies": {
        "react": ">=16.8.1",
        "react-native": ">=0.60.0-rc.0 <1.0.x",
        "react-native-vector-icons": "^9.2.0"
    },
    "devDependencies": {
        "@brightlayer-ui/icons": "^2.0.0",
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "@types/react-native-vector-icons": "^6.4.12",
        "cross-env": "^7.0.3",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^3.2.5",
        "react": "18.1.0",
        "react-native": "0.70.0",
        "react-native-vector-icons": "^9.2.0",
        "typescript": "^4.8.3"
    },
    "prettier": "@brightlayer-ui/prettier-config"
}
